{
    "env": {
        "browser": true,
        "es2021": true,
        "node":true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-var":"error",
        "linebreak-style": ["error", "windows"],
        "semi":["error","never"],
        "no-console":"error",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}
